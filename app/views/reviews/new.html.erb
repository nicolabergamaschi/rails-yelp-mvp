<h1>Reviews#new</h1>
<p>Find me in app/views/reviews/new.html.erb</p>

<div class="main-div">
  <h1 class="title">New Review</h1>
  <div class="form-new">
    <%= simple_form_for([@restaurant, @review]) do |f|%>
      <%= f.input :content %>
      <%= f.input :rating, as: :integer, input_html: { min: 1, max: 5 } %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
